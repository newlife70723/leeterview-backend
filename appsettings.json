{
  "ConnectionStrings": {    
    "DefaultConnection": "Server=${DB_HOST};Database=${DB_NAME};User Id=${DB_USER};Password=${DB_PASSWORD};TrustServerCertificate=True;",
    "RedisConnection": "${REDIS_HOST}:${REDIS_PORT},password=${REDIS_PASSWORD}"
  },

  "AWS": {
    "Region": "${AWS_REGION}",
    "AccessKey": "${AWS_ACCESS_KEY}",
    "SecretKey": "${AWS_SECRET_KEY}"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
